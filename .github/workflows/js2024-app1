function isString(data) {
    return typeof data = 'string' 
  }
  console.log("Exercice 1 : ");
  console.log(isString('hello65656'));
  console.log(isString([1,6,9])); 
  console.log(isString(5)); 
  console.log(isString("Is54545et")); 

