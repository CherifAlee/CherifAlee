/*exercice 4
function extractString(str1, length) {
  return str1.substring(0, length);
}

console.log(extractString("Robin Singh", 4)); // "Robi"
console.log(extractString("ISET Tataouine", 7)); // "ISET Ta"
*/
/*exercice5
function protectEmail(user_email) {
  const emailLength = user_email.length;
  const visiblePartLength = 4;
  const visiblePart = user_email.substring(0, visiblePartLength);
  const hiddenPart = "..." + user_email.substring(emailLength - visiblePartLength, emailLength);
  return visiblePart + hiddenPart;
}

console.log(protectEmail("isettataouine@gmail.com"));
